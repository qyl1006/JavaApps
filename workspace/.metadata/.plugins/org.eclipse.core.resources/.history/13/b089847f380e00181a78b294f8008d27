package cn.wolfcode.smis.util;

import javax.sql.DataSource;

//提供连接池对象的工具类
public abstract class JdbcUtil {
	private static DataSource ds;
	
	//静态代码块
	static {
		try {
			//读取属性文件, 获取4要素信息
			ClassLoader loader = JdbcUtil.class.getClassLoader();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
