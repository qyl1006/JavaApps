package com._520it.Set;
//演示自然排序  与 定制排序
public class TreeSetDemo {

}

//定义一个类
class Student implements Comparable<Student>{
	private String name;
	private int age;
	
	Student(String name, int age){
		this.name = name;
		this.age = age;
	}
	
	public int compareTo(Student other) {
		if(this.age > other.age){
			return 1;
		}else if(this.age < this.age){
			return -1;
		}
		return 0;
	}
	
	
}
