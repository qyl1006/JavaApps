package com._520it.day02;

//Runnable实现类---生产类
public class ProducerDemo implements Runnable {
	private Middle m;
	
	//通过构造器来调用Middle的对象(共享资源类)-----构造器的用法吧----学习了
	ProducerDemo(Middle m){
		this.m = m;
	}
	
	//一个线程--->调用Middle类的生产方法
	public void run() {
		for (int i = 0; i < 50; i ++){
			m.production("凤姐", "女");
		}
	}
	

}
