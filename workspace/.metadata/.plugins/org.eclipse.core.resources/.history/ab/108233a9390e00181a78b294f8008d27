package cn.wolfcode.smis.util;

import java.sql.Connection;

import lombok.Cleanup;

//一个提供模板的工具类
public abstract class JdbcTemplate {
	//DML模板
	public static void update(String sql, Object...params){
		try {
			//获取连接对象
			@Cleanup
			Connection conn = JdbcUtil.getConn();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
