package cn.wolfcode.smis.dao;

import cn.wolfcode.smis.domain.Student;

/**
 * 操作学生对象的DAO接口
 * @author Qyuelin
 *
 */
public interface IStudentDAO {
	/**
	 * 保存一个学生信息
	 * @param stu 被保存的学生对象
	 */
	void save(Student stu);
	
	/**
	 * 更新一个学生信息
	 * @param stu 被更新的学生对象信息
	 */
	void update(Student stu);
}
