package day18.exception_class;
/*
 * 自定义异常类
 */

public class LogicException extends RuntimeException {

	public LogicException() {
		super();
	}

	public LogicException(String message) {
		super(message);
	}
	public LogicException(String message, Throwable cause) {
		super(message, cause);

	}
	
}
