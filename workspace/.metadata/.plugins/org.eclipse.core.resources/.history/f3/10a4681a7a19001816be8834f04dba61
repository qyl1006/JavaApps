import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

public class Test {
	public static void main(String[] args) {
		A a = new A();
		B test = new B();
		test(test);
	}
	
	public static void test(A a){
		System.out.println(a); //
	}
}

@Setter @Getter @ToString
class A{
	private String a = "AAAA";
}

@Setter @Getter @ToString
class B extends A{
	private String b = "BB";
}