
public class Test5 {
	public static void main(String[] args) {
		char[] c = new char[]{'A','B','C','D'};
		char[] c2 = new char[]{'C', 'D'};
		
		for (int i = 0; i < c.length; i++) {
			int temp = c2.length;
			int j = 0;
			int ii = i;
			while(c[ii] == c2[j]){
				ii ++;
				j ++;
				if(j == c2.length -1){
					System.out.println(i);
				}
			}
		}
	}
}
