package day19;

/*
 * 用实现方式 演示吃苹果比赛的案例
 */
public class ImplementsDemo {
	public static void main(String[] args) {
		
	}
}

//定义一个实现的类 
class Abs implements Runnable{
	private int apple = 50;
	
	public void run(){
		for (int i = 0; i < 50; i++) {
			if(apple > 0){
				System.out.println(Thread.currentThread().getName() + "展示并迅速地吃了" + apple + "号苹果");
			}
		}
		
	}
}