package com._520it.day02_demo;

import java.io.File;
import java.io.FilenameFilter;
import java.io.InputStream;

public class Demo {
	public static void main(String[] args) {
		File in = new File("C:/Users/Qyuelin/Desktop/java_test/infile");
		File out = new File ("C:/Users/Qyuelin/Desktop/java_test/outfile");
		File[] fs = in.listFiles(new FilenameFilter(){
			public boolean accept(File dir, String name) {
				return new File(dir, name).isFile() && name.endsWith(".txt");
			}
			
		});
		
		byte[] b = new byte[5];
		for(File file : fs){
			InputStream in = new FileInputStream(in);
		}
	}
}
