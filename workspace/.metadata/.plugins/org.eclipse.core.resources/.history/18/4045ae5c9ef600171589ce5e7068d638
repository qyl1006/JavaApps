
public class Test7 {
	public static void main(String[] args) {
		System.out.println(test());
		
		Integer i = 1;
		System.out.println(i);
		
		++ i;
		System.out.println(i);
	}

	private static Integer test() {
		Integer i = 1;
		try{
			return i;
		}finally{
			++ i;
		}
		System.out.println(i);
	}
}
