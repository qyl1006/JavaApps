package com._520it.day01_InetAddress;

import java.net.InetAddress;
import java.net.UnknownHostException;

/*
 * ¡∑œ∞InetAddress¿‡
 */
public class InetAddressDemo {
	public static void main(String[] args) throws Exception {
		InetAddress[] ip = InetAddress.getAllByName("www.baidu.com");
		System.out.println(ip[0]);
		System.out.println(ip[0].getHostAddress());
		System.out.println(ip[0].getHostName());
		System.out.println(ip[0].isReachable(100000));
	}
}
