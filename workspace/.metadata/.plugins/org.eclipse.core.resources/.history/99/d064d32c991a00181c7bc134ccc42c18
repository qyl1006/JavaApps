package com.qinyuelin.qyl.dao;

import com.qinyuelin.qyl.domain.Player;

/**
 * 处理Player类的DAO接口
 * @author Qyuelin
 *
 */
public interface IPlarerDAO {
	/**
	 * 增加一个游戏玩家信息
	 * @param p 被增加玩家对象
	 */
	void save(Player p);
	
	/**
	 * 更新一个玩家信息
	 * @param p 被更新的玩家对象
	 */
	void update(Player p);
	
	void delete(Long id);
}
